{
    function fibonacci(a : Integer) : Integer {
        if a = 1 then return with 0
        if a = 2 then return with 1
        return with fibonacci(a-1) + fibonacci(a-2)
    }

    procedure print_fibonacci {
        var i : Integer
        i := 1
        while (i < 6) do {
            write fibonacci(i), newline
            i := i + 1
        }
    }

    print_fibonacci
}