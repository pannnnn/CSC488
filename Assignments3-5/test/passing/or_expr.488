{
    var a, b, c, d : Boolean
    procedure print_boolean(x : Boolean) {
        if x then write "true", newline else write "false", newline
    }

    a := true or true
    b := true or false
    c := false or true
    d := false or false

    print_boolean(a)
    print_boolean(b)
    print_boolean(c)
    print_boolean(d)

}
